# secrets.yaml.template - 敏感信息配置模板
# 复制此文件为 secrets.yaml 并填入真实的 API 密钥

# AI API配置
ai:
  # Kimi API配置
  kimi:
    api_key: "YOUR_KIMI_API_KEY_HERE"  # 从 https://platform.moonshot.cn/ 获取
    base_url: "https://api.moonshot.cn/v1"

  # 其他AI提供商配置（可选）
  deepseek:
    api_key: ""
    base_url: "https://api.deepseek.com"

  openai:
    api_key: ""
    base_url: "https://api.openai.com/v1"

# 飞书推送配置
feishu:
  webhook_url: "YOUR_FEISHU_WEBHOOK_URL_HERE"  # 从飞书机器人设置获取

# 其他敏感配置（可选）
database:
  host: ""
  port: ""
  username: ""
  password: ""
